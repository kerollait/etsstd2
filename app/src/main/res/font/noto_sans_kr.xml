<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools">

    <font
        android:font="@font/noto_sans_kr_medium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:fontStyle="normal"
        app:font="@font/noto_sans_kr_medium"
        app:fontWeight="500"
        tools:ignore="UnusedAttribute"/>

    <font
        android:font="@font/noto_sans_kr_bold"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:fontStyle="normal"
        app:font="@font/noto_sans_kr_bold"
        app:fontWeight="700"
        tools:ignore="UnusedAttribute"/>

    <font
        android:font="@font/noto_sans_kr_black"
        android:fontStyle="normal"
        android:fontWeight="900"
        app:fontStyle="normal"
        app:font="@font/noto_sans_kr_black"
        app:fontWeight="900"
        tools:ignore="UnusedAttribute"/>

</font-family>